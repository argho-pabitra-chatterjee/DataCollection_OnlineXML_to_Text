<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="PubmedArticleSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PubmedArticle" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="MedlineCitation">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="PMID">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:int">
                            <xs:attribute type="xs:byte" name="Version" use="optional"/>
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="DateCreated">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:short" name="Year"/>
                          <xs:element type="xs:byte" name="Month"/>
                          <xs:element type="xs:byte" name="Day"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="DateRevised">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:short" name="Year"/>
                          <xs:element type="xs:byte" name="Month"/>
                          <xs:element type="xs:byte" name="Day"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Article">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Journal">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="ISSN">
                                  <xs:complexType>
                                    <xs:simpleContent>
                                      <xs:extension base="xs:string">
                                        <xs:attribute type="xs:string" name="IssnType" use="optional"/>
                                      </xs:extension>
                                    </xs:simpleContent>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="JournalIssue">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element type="xs:byte" name="Volume" minOccurs="0"/>
                                      <xs:element type="xs:byte" name="Issue" minOccurs="0"/>
                                      <xs:element name="PubDate">
                                        <xs:complexType>
                                          <xs:sequence>
                                            <xs:element type="xs:short" name="Year"/>
                                            <xs:element type="xs:string" name="Month"/>
                                            <xs:element type="xs:byte" name="Day"/>
                                          </xs:sequence>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                    <xs:attribute type="xs:string" name="CitedMedium" use="optional"/>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element type="xs:string" name="Title"/>
                                <xs:element type="xs:string" name="ISOAbbreviation"/>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element type="xs:string" name="ArticleTitle"/>
                          <xs:element name="Pagination" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element type="xs:string" name="MedlinePgn"/>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="ELocationID" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="xs:string" name="EIdType" use="optional"/>
                                  <xs:attribute type="xs:string" name="ValidYN" use="optional"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="Abstract">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element type="xs:string" name="AbstractText"/>
                                <xs:element type="xs:string" name="CopyrightInformation" minOccurs="0"/>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="AuthorList">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Author" maxOccurs="unbounded" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element type="xs:string" name="LastName"/>
                                      <xs:element type="xs:string" name="ForeName"/>
                                      <xs:element type="xs:string" name="Initials"/>
                                      <xs:element name="AffiliationInfo">
                                        <xs:complexType>
                                          <xs:sequence>
                                            <xs:element type="xs:string" name="Affiliation"/>
                                          </xs:sequence>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                    <xs:attribute type="xs:string" name="ValidYN" use="optional"/>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute type="xs:string" name="CompleteYN" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                          <xs:element type="xs:string" name="Language"/>
                          <xs:element name="PublicationTypeList">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="PublicationType">
                                  <xs:complexType>
                                    <xs:simpleContent>
                                      <xs:extension base="xs:string">
                                        <xs:attribute type="xs:string" name="UI" use="optional"/>
                                      </xs:extension>
                                    </xs:simpleContent>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="ArticleDate">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element type="xs:short" name="Year"/>
                                <xs:element type="xs:byte" name="Month"/>
                                <xs:element type="xs:byte" name="Day"/>
                              </xs:sequence>
                              <xs:attribute type="xs:string" name="DateType" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute type="xs:string" name="PubModel" use="optional"/>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MedlineJournalInfo">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:string" name="Country"/>
                          <xs:element type="xs:string" name="MedlineTA"/>
                          <xs:element type="xs:int" name="NlmUniqueID"/>
                          <xs:element type="xs:string" name="ISSNLinking" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="KeywordList" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Keyword" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="xs:string" name="MajorTopicYN" use="optional"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute type="xs:string" name="Owner"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute type="xs:string" name="Status" use="optional"/>
                  <xs:attribute type="xs:string" name="Owner" use="optional"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="PubmedData">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="History">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="PubMedPubDate" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element type="xs:short" name="Year"/>
                                <xs:element type="xs:byte" name="Month"/>
                                <xs:element type="xs:byte" name="Day"/>
                                <xs:element type="xs:byte" name="Hour"/>
                                <xs:element type="xs:byte" name="Minute"/>
                              </xs:sequence>
                              <xs:attribute type="xs:string" name="PubStatus" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element type="xs:string" name="PublicationStatus"/>
                    <xs:element name="ArticleIdList">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ArticleId" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="xs:string" name="IdType" use="optional"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>